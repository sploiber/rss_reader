<h2>Channel of <%= @profile.name %></h2>
<h3>Title: <%= @channel.title %></h3>
<h3>URL: <%= @channel.url %></h3>
<%= link_to "Edit Channel", [:edit, @profile, @channel], :class => "btn btn-primary btn-large" %>
<%= link_to "Delete Channel", [@profile, @channel], :method => :delete,
  :confirm => "Are you sure you want to delete this channel?", :class => "btn btn-primary btn-large" %>
<%= link_to "Return to Profile", :action => "index", :controller => "home", :class => "btn btn-primary btn-large" %>
